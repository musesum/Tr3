sky.draw {
    brush {
        type  : "dot"
        size  : (1...48=1) <-(input.force input.radius cell~brush.size) //<-(osc.tuio.next~f  osc.manos~z)
        press : (0...1=1)
        tilt  : (0...1=0)
        index : (1...255=127) //<-(osc.tuio.z osc.manos~z)
        
        color {
            hue : (0...360)
            sat : (0...100=100)
            val : (0...100=100)
            min : (0...255=0)
            max : (0...255=255)
        }
    }
    note {
        circle {
            number   : (0...127)
            velocity : (0...127)
            channel  : (1...16)
            duration
            sustain
            t
        }
        square {
            number   : (0...127) <-(osc.midi.note.number)
            velocity : (0...127) <-(osc.midi.note.velocity)
            channel  : (1...16)  <-(osc.midi.note.channel)
            duration             <-(osc.midi.note.duration)
            sustain
            t                    <-(osc.midi.note)
        }
    }
    shape {
        dot {
            next   : (x y):(0...1)
            radius : (1...64=16)
            color  : (0...255=255)
        }
        line {
            prev   : (x y):(0...1) <-(osc.tuio.prev.* osc.manos.*)
            next   : (x y):(0...1) <-(osc.tuio.next.* osc.manos.*)
        }
        rect {
            prev  : (x y):(0...1)
            next  : (x y):(-0.25...0.25)
            index : (0...255=127)
        }
    }
}
