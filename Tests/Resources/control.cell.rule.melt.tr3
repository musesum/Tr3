control.cell.rule.melt : _controlSpeed {
    base {
        title : "Melt"
        icon  : "control.cell.rule.melt.png"
    }
    shader {
        cellMelt : "cell.rule.melt.metal" {
            version : (0...1=0) <- elements.version.value
            on      : (0...1=0) <- elements.ruleOn.value
            type    : "compute"
        }
    }
    elements {
        ruleOn.icon : "control.cell.rule.melt.png"
    }
}
