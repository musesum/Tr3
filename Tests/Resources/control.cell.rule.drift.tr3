control.cell.rule.drift : _controlSpeed {
    base {
        title : "Drift"
        icon  : "control.cell.rule.drift.png"
    }
    shader {
        cellDrift : "cell.rule.drift.metal" {
            version : (0...1=0) <- elements.version.value
            on : (0...1=0) <- elements.ruleOn.value
        }
    }
    elements {
        ruleOn.icon   : "control.cell.rule.drift.png"
    }
}
