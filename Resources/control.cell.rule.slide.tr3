control.cell.rule.slide : _controlSpeed {
    base {
        title : "Slide Bit Planes"
        icon  : "control.cell.rule.slide.png"
    }
    shader {
        cellSlide : "cell.rule.slide.metal" {
            version : (0...1=0) <- elements.version.value
            on      : (0...1=0) <- elements.ruleOn.value
            type    : "compute"
        }
    }
    elements {
        ruleOn.icon   : "control.cell.rule.slide.png"
    }
}
